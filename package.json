{
  "name": "@aion-dk/js-client",
  "version": "0.0.71425",
  "description": "Assembly Voting JS client",
  "main": "dist/lib/av_client.js",
  "types": "dist/lib/av_client.d.ts",
  "scripts": {
    "test": "mocha --require ts-node/register/transpile-only --require source-map-support/register --recursive --extension ts ./test/*.test.ts",
    "coverage": "tsc && nyc --reporter=json-summary --reporter=text npm run test",
    "tdd": "mocha --require ts-node/register/transpile-only --require source-map-support/register --extension ts ./test/**/*.test.ts --watch --watch-files test/**/*.ts,lib/**/*.ts",
    "docs": "typedoc --includes test/",
    "html_docs": "typedoc --plugin none --out html_docs --includes test/ --theme typedoc-wide-theme",
    "prepublish": "tsc && typedoc --includes test/",
    "build": "tsc && cp lib/av_client/*.js dist/lib/av_client",
    "lint": "eslint lib/"
  },
  "dependencies": {
    "axios": "^0.21.4"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^8.2.3",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "mocha": "^9.0.1",
    "nock": "^13.1.3",
    "nyc": "^15.1.0",
    "sinon": "^11.1.2",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.0.0",
    "typedoc": "^0.21.2",
    "typedoc-plugin-markdown": "^3.10.2",
    "typescript": "^4.3.5"
  }
}
